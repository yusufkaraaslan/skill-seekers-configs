{
  "name": "astro",
  "description": "Astro web framework for content-focused websites. Use for Astro components, islands architecture, content collections, SSR/SSG, and modern web development.",
  "base_url": "https://docs.astro.build/en/getting-started/",
  "start_urls": [
    "https://docs.astro.build/en/getting-started/",
    "https://docs.astro.build/en/install/auto/",
    "https://docs.astro.build/en/core-concepts/project-structure/",
    "https://docs.astro.build/en/core-concepts/astro-components/",
    "https://docs.astro.build/en/core-concepts/astro-pages/"
  ],
  "selectors": {
    "main_content": "article",
    "title": "h1",
    "code_blocks": "pre code"
  },
  "url_patterns": {
    "include": ["/en/"],
    "exclude": ["/blog/", "/showcase/"]
  },
  "categories": {
    "basics": ["basics", "getting-started", "install", "tutorial", "project"],
    "components": ["components", "props", "slots", "astro-components"],
    "routing": ["routing", "pages", "endpoints", "dynamic"],
    "integrations": ["integration", "framework", "adapter", "react", "vue", "svelte"],
    "content": ["markdown", "mdx", "content-collections", "content"],
    "islands": ["islands", "client", "hydration", "partial"],
    "deployment": ["deploy", "adapter", "host", "vercel", "netlify"]
  },
  "rate_limit": 0.5,
  "max_pages": 250
}